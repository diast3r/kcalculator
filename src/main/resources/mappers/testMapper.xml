<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kcalculator.test.mapper.TestMapper">
	<select id="selectIngredientList" parameterType="int" resultType="com.kcalculator.test.domain.ProcessedIngredient">
		SELECT
			*
		FROM
			`ingredient_processed`
		LIMIT 1000
		OFFSET #{offset}
	</select>

</mapper>